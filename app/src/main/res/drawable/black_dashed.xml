<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item  >
        <rotate
            android:fromDegrees="0"
            android:toDegrees="90"
            android:pivotX="0%"
            android:pivotY="0%">
            <shape
                android:shape="rectangle"   >
                <solid
                    android:color="#F5F7FC"  />
                <stroke
                    android:width="1dp"
                    android:color="#403743BE" />
            </shape>

        </rotate>
    </item>
</layer-list>