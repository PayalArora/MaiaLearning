                android:textSize="@dimen/_14sp" />

            <TextView
                android:id="@+id/text_submitted"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginVertical="@dimen/_5dp"
                android:layout_marginStart="@dimen/_5dp"
                android:layout_toRightOf="@+id/text_carr"
                android:background="@drawable/univ_rounded_icon"
                android:backgroundTint="@color/green_1"
                android:drawableStart="@drawable/arrow_up"
                android:drawablePadding="@dimen/_5dp"
                android:fontFamily="@font/inter_bold"
                android:padding="@dimen/_5dp"
                android:text="@string/submitted"
                android:textColor="@color/green_1"
                android:textSize="@dimen/_12sp" />

            <ImageButton
                android:layout_width="@dimen/_10dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_marginRight="@dimen/_15dp"
                android:background="@android:color/transparent"
                android:src="@drawable/menu_dots" />

            <TextView
                android:id="@+id/sub_description"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_carr"
