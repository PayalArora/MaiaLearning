                        android:fontFamily="@font/inter"
                        android:lineSpacingExtra="@dimen/_2sp"
                        android:text="@string/five_docs"
                        android:textColor="@color/univ_name"
                        android:textSize="@dimen/_14sp" />

                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/fair_lay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/_5dp"
        android:visibility="gone"
        app:cardCornerRadius="@dimen/_18dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/_5dp">

            <TextView
                android:id="@+id/text_fair"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginVertical="@dimen/_10dp"
                android:drawableStart="@drawable/ic_fair"
                android:drawablePadding="@dimen/_5dp"
                android:drawableTint="@color/blue"
                android:fontFamily="@font/inter_bold"
                android:paddingStart="@dimen/_10dp"
                android:text="@string/fair_or_webinar"
                android:textColor="@color/blue"
                android:textSize="@dimen/_14sp" />


            <ImageButton
                android:layout_width="@dimen/_10dp"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginTop="@dimen/_5dp"
                android:layout_marginRight="@dimen/_15dp"
                android:background="@android:color/transparent"
                android:src="@drawable/menu_dots" />

            <TextView
                android:id="@+id/fair_description"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/text_fair"
                android:layout_marginHorizontal="@dimen/_15dp"
                android:layout_marginTop="@dimen/_5dp"
                android:fontFamily="@font/inter"
                android:lineSpacingExtra="@dimen/_2sp"
                android:text="@string/san_franc"
                android:textColor="@color/univ_name"
                android:textSize="@dimen/_14sp" />

            <View
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_05dp"
                android:layout_below="@id/fair_description"
                android:layout_marginTop="@dimen/_10dp"
                android:background="@color/grey_1" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/view3"
                android:orientation="horizontal"
                android:padding="@dimen/_10dp">

                <ImageView
                    android:id="@+id/marker"
                    android:layout_width="@dimen/_25dp"
                    android:layout_height="@dimen/_25dp"
                    android:layout_centerVertical="true"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_marker" />

                <RelativeLayout
                    android:id="@+id/address_lay"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toLeftOf="@+id/time_text"
                    android:layout_toRightOf="@+id/marker"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/text_address"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:layout_marginHorizontal="@dimen/_15dp"
                        android:layout_marginTop="@dimen/_5dp"
                        android:fontFamily="@font/inter"
                        android:lineSpacingExtra="@dimen/_2sp"
                        android:text="@string/address"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_14sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/text_address"
                        android:layout_marginHorizontal="@dimen/_15dp"
                        android:layout_marginTop="@dimen/_5dp"
                        android:fontFamily="@font/inter"
                        android:lineSpacingExtra="@dimen/_2sp"
                        android:text="@string/san_francisco"
                        android:textColor="@color/univ_name"
                        android:textSize="@dimen/_14sp" />

                </RelativeLayout>

                <TextView
                    android:id="@+id/time_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_centerVertical="true"
                    android:layout_gravity="end"
                    android:layout_marginHorizontal="@dimen/_15dp"
                    android:layout_marginTop="@dimen/_5dp"
                    android:background="@drawable/white_rounded"
                    android:backgroundTint="@color/alpha_blue_t"
                    android:fontFamily="@font/inter"
                    android:lineSpacingExtra="@dimen/_2sp"
                    android:padding="@dimen/_5dp"
                    android:text="@string/time"
                    android:textColor="@color/alpha_blue1"
                    android:textSize="@dimen/_14sp" />
            </RelativeLayout>
        </RelativeLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
