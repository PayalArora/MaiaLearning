<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white_1">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/_10dp"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.cardview.widget.CardView
                android:id="@+id/layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                app:cardCornerRadius="@dimen/_10dp"
                app:layout_constraintTop_toTopOf="parent">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_20dp"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/about"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:text="@string/about"
                    android:textColor="@color/black"
                    android:textSize="16sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/about_"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20dp"
                    android:fontFamily="@font/inter_regular"
                    android:lineSpacingExtra="2sp"
                    android:text="@string/about_uni"
                    android:textColor="@color/black_1"
                    android:textSize="13sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/about" />

                <TextView
                    android:id="@+id/web"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20dp"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:text="@string/web"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/about_" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_20dp"
                    android:lineSpacingExtra="2sp"
                    android:text="http://www.usyd.edu.au"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@id/web"
                    app:layout_constraintTop_toBottomOf="@id/about_" />

                <TextView
                    android:id="@+id/phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:text="@string/phone"
                    android:layout_marginTop="@dimen/_5sp"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/web" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="2sp"
                    android:text="+61(2) 9351-2222"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    android:layout_marginTop="@dimen/_5sp"
                    app:layout_constraintStart_toEndOf="@id/phone"
                    app:layout_constraintTop_toBottomOf="@id/web" />

                <TextView
                    android:id="@+id/enviro"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:layout_marginTop="@dimen/_5sp"
                    android:text="Type of Environment:"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/phone" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="2sp"
                    android:text="N/A"
                    android:layout_marginTop="@dimen/_5sp"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@id/enviro"
                    app:layout_constraintTop_toBottomOf="@id/phone" />

                <TextView
                    android:id="@+id/public_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:layout_marginTop="@dimen/_5sp"
                    android:text="Public Term Type:"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/enviro" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="2sp"
                    android:text=" N/A"
                    android:layout_marginTop="@dimen/_5sp"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@id/public_type"
                    app:layout_constraintTop_toBottomOf="@id/enviro" />

                <TextView
                    android:id="@+id/types"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:layout_marginTop="@dimen/_5sp"
                    android:text="Type of Degrees Offered:"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/public_type" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:lineSpacingExtra="2sp"
                    android:text="N/A"
                    android:layout_marginTop="@dimen/_5sp"
                    android:textColor="@color/black_1"
                    android:textSize="14sp"
                    app:layout_constraintStart_toEndOf="@id/types"
                    app:layout_constraintTop_toBottomOf="@id/public_type" />
            </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
            <FrameLayout
                android:id="@+id/map_"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20dp"
                android:background="@drawable/background_curve_rectangle_white_filled"
                android:padding="@dimen/_5dp"
                app:layout_constraintTop_toBottomOf="@id/layout">

                <FrameLayout
                    android:id="@+id/map"
                    android:name="com.google.android.gms.maps.SupportMapFragment"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10dp"
                    android:layout_marginTop="@dimen/_10dp"
                    android:fontFamily="@font/inter_bold"
                    android:lineSpacingExtra="2sp"
                    android:text="Location"
                    android:textColor="@color/black"
                    android:textSize="18sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
            </FrameLayout>

            <View
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20dp"
                android:background="@color/tint"
                app:layout_constraintTop_toBottomOf="@id/map_" />

            <TextView
                android:id="@+id/textVideo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_10dp"
                android:layout_marginTop="@dimen/_20dp"
                android:fontFamily="@font/inter_bold"
                android:lineSpacingExtra="2sp"
                android:text="Videos"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/view" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recycler_View"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textVideo" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>