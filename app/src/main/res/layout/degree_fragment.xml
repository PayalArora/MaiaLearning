<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/location"
        android:layout_width="0dp"
        android:layout_height="@dimen/_30dp"
        android:layout_marginStart="@dimen/_30dp"
        android:layout_marginTop="@dimen/_20dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="Programs"
  android:gravity="center|start"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@id/aa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/aa"
        android:layout_width="@dimen/_30dp"
        android:layout_height="@dimen/_30dp"
        android:layout_marginTop="@dimen/_20dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="AA"
        android:gravity="center"
        android:padding="@dimen/_5dp"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:layout_marginEnd="@dimen/_10dp"
        app:layout_constraintEnd_toStartOf="@id/ba"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/ba"
        android:layout_width="@dimen/_30dp"
        android:layout_height="@dimen/_30dp"
        android:layout_marginTop="@dimen/_20dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="BA"
        android:gravity="center"
        android:layout_marginEnd="@dimen/_10dp"
        android:padding="@dimen/_5dp"
        android:textColor="@color/black"
        android:textSize="12sp"
        app:layout_constraintEnd_toStartOf="@id/ma"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/ma"
        android:layout_width="@dimen/_30dp"
        android:layout_height="@dimen/_30dp"
        android:layout_marginTop="@dimen/_20dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="MA"
        android:gravity="center"
        android:padding="@dimen/_5dp"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:layout_marginEnd="@dimen/_10dp"
        app:layout_constraintEnd_toStartOf="@id/da"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/da"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="@dimen/_20dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="DA"
        android:gravity="center"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:layout_marginEnd="@dimen/_10dp"
        app:layout_constraintEnd_toStartOf="@id/dda"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/dda"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="@dimen/_10dp"
        android:fontFamily="@font/inter_bold"
        android:lineSpacingExtra="2sp"
        android:text="DA"
        android:padding="@dimen/_5dp"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:visibility="invisible"
        android:layout_marginEnd="@dimen/_20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.recyclerview.widget.RecyclerView
    android:id="@+id/recyclerView"
    android:layout_width="0dp"
    android:layout_height="@dimen/_0dp"
        android:layout_marginStart="@dimen/_20dp"
        android:layout_marginEnd="@dimen/_20dp"
    app:layout_constraintTop_toBottomOf="@id/da"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>