<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:background="@color/color_acent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:padding="@dimen/_10dp"
        android:layout_height="@dimen/_100dp">
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/ic_dashboard"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/dashboard"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:textSize="@dimen/_14sp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:layout_marginLeft="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/ic_character"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/character"
                android:textColor="@color/white"
                android:textSize="@dimen/_14sp"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10dp"

            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/career"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/careers"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:textSize="@dimen/_14sp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:padding="@dimen/_10dp"
        android:layout_height="@dimen/_100dp">
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/academic"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/academics"
                android:textColor="@color/white"
                android:textSize="@dimen/_14sp"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:layout_marginLeft="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/university"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/universities"
                android:textColor="@color/white"
                android:textSize="@dimen/_14sp"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10dp"

            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/event"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/events"
                android:textSize="@dimen/_14sp"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:padding="@dimen/_10dp"
        android:layout_height="@dimen/_100dp">
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/portfolio"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/portfolio"
                android:textSize="@dimen/_14sp"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:layout_marginLeft="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/mala"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/maiadrive"
                android:textColor="@color/white"
                android:textSize="@dimen/_14sp"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10dp"

            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/calender"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/calender"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:textSize="@dimen/_14sp"
                android:layout_height="wrap_content"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:padding="@dimen/_10dp"
        android:layout_height="@dimen/_100dp">
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/ic_res"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/resources"
                android:textColor="@color/white"
                android:textSize="@dimen/_14sp"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:padding="@dimen/_10dp"
            android:layout_marginLeft="@dimen/_10dp"
            android:background="@drawable/rectangle"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">
            <ImageView
                android:layout_width="@dimen/_20dp"
                android:src="@drawable/about"
                android:layout_height="@dimen/_20dp"/>

            <TextView
                android:layout_width="match_parent"
                android:text="@string/about"
                android:textColor="@color/white"
                android:layout_marginTop="@dimen/_15dp"
                android:layout_height="wrap_content"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/_10dp"
            android:padding="@dimen/_10dp"
            android:layout_gravity="center_vertical"
            android:layout_height="match_parent">

        </LinearLayout>

    </LinearLayout>
</LinearLayout>